{
    "name": "chubbyphp/chubbyphp-framework-skeleton",
    "description": "A minimal skeleton for chubbyphp-framework",
    "keywords": ["chubbyphp", "framework", "skeleton"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "chubbyphp/chubbyphp-config": "^1.2",
        "chubbyphp/chubbyphp-framework": "^2.3",
        "nikic/fast-route": "^1.3",
        "pimple/pimple": "^3.2.3",
        "slim/psr7": "^0.6",
        "symfony/console": "^4.3.4"
    },
    "require-dev": {
        "chubbyphp/chubbyphp-mock": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.15.3",
        "infection/infection": "^0.14.1",
        "phploc/phploc": "^5.0",
        "phpstan/phpstan": "^0.11.16",
        "phpunit/phpunit": "^8.3.5"
    },
    "autoload": {
        "psr-4": { "App\\": "app/" }
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
